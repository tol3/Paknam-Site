
<div class="row">
  <div class="col-sm-10 col-sm-offset-1">
    <div class="bs-example">
        <table class="table">
          <thead>
            <tr>
              <th>หัวข้อเอกสาร</th>
              <th>วันที่</th>
            </tr>
          </thead>
          <tbody>
            <% @news.each do |d| %>
              <tr>
                <td><%= link_to(d.title, news_path(d.id)) %></td>
                <td><%= d.created_at.strftime("%d-%m-%Y") %></td>
              </tr>
          <% end %>
          </tbody>
        </table>
    </div>
  </div>
</div>
  