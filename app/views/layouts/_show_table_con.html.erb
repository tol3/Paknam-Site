<div class="row">
     <div class = "col-sm-10 col-sm-offset-1"> <h2 class ="center"> <%= @doc.title %></h2> </div>
</div>
<div class="row">
     <div class ="col-sm-10 col-sm-offset-1">  <h4 class = "right"> วันที่: <%= @doc.created_at.strftime("%d-%m-%Y") %></h4></div>
</div>
<div class="row">
     <div class="col-sm-10 col-sm-offset-1">   <h4><%= @doc.category %> </h4> </div>
</div>
<div class="row">
     <div class="col-sm-10 col-sm-offset-1">   <p class="news">  <%= raw @doc.content %>  </p></div>
</div>
<div class="row">
     <div class="col-sm-10 col-sm-offset-1">  <h4> <%= link_to("<i class='fa fa-file-pdf-o fix-i'></i> Download PDF".html_safe, @doc.file.url) %> </h4>
     </div>
</div>
<div class="row">
    <div class="col-sm-10 col-sm-10-offset-1">
       <h4 class="right"> <%= link_to("<i class='fa fa-chevron-left'></i> Back".html_safe, :back ) %></h4>
    </div>
</div>
