<!DOCTYPE html>
<html>
	<head>
		<title>Paknam Site | <%= yield(:title) || "City" %> </title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div class="wraper"></div>
		<div class="huhu">
			<header>
				<%= render 'layouts/header' %>
			</header>
			<div class="row first">
				<div class="custom-col add-bg">
					<%= yield %>
				</div>
			</div>
			<footer>
				<%= render 'layouts/footer' %>
			</footer>
		</div>
	</body>
</html>
