<!DOCTYPE html>
<html>
	<head>
		<title>Paknam Site | <%= yield(:title) || "City" %> </title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div class="wraper"></div>
			<div class="huhu  head-bg">
				<header>
					<div class="row no-pad">
						<div class="col-md-10 col-md-offset-1 no-pad  head-box">
							<%= render 'layouts/header' %>	
						</div>
					</div>
				</header>

				<div class="row">
					<div class="col-md-10 col-md-offset-1 ss">
						<%= yield %>
					</div>
				</div>
			</div>
			<footer>
				<%= render 'layouts/footer' %>
			</footer>
	</body>
</html>
